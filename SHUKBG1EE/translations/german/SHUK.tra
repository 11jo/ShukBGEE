@8001 = ~Idiot!~
@8002 = ~Fieser Mistkäfer!~
@8003 = ~Stinkendes Fischgesicht!~
@8004 = ~Schwitzender Fettsack!~
@8005 = ~Räudige Zwergenschlampe!~
@8006 = ~Feuchter Trollfladen!~
@8007 = ~Hässliche Koboldfresse!~
@8008 = ~Elender Menschensohn!~
@8009 = ~Dreckiger Elfenarsch!~
@8010 = ~Sabbernde Ghulbacke~
@8011 = ~Das reicht, Drecksack!~
@8012 = ~frisst Proviant~
@8013 = ~Shuk ist wütend!~
@8100 = ~Ich bin müde, Holzkopf! Schick mich wieder in den Stein!~
@8101 = ~Hey Affengesicht, ich muss bald in den Stein zurück!~
@8102 = ~Lass mich wieder in Stein, dämliche Made!~
@8103 = ~Eiterbacke! Ich muss in den Stein!~
@8104 = ~Ich musste Dich lange genug ertragen, Mastschwein! Jetzt lass mich in den Stein!~
@8105 = ~Bist du blind oder nur dumm, Dumpfnase? Der Stein ruft nach mir!~
@8106 = ~Elender, ich werde müde! Lass mich in den Stein!~
@8107 = ~Bah! Zahnloser Krötenpiester! Der Stein ruft!~
@8108 = ~Fettklopps! Schick mich endlich wieder in den Stein!~
@8109 = ~Du! Glubschauge! Ich will in den Stein. JETZT!~
@8110 = ~Na endlich!~
@8111 = ~Soll das Gift sein, Kniepisser!? Lächerlich!~
@8112 = ~Ich hasse diese dreckigen Steine.~
@8113 = ~Ach du Scheiße!~
@8114 = ~*Gähnt*~
