// Raki Skill

IF
  Name("NQ#GB18",Myself)
  See(Nearestenemyof(Myself))
  HaveSpellRES("SPCL321")
  Global("Angry","LOCALS",0)
  OR(2)
   Global("NQ#SHUKTACTICS","GLOBAL",0)
   Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    SetInterrupt(FALSE)
    SpellRES("SPCL321", Myself)
	SetGlobal("Angry","LOCALS",1)
	SetInterrupt(TRUE)
END

